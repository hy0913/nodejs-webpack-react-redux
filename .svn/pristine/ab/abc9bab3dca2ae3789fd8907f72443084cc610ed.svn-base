  		let timeObject = timeToString(dateString);
  		this.state.ec_ranges_start = timeObject.startTime;
  		this.state.ec_ranges_end = timeObject.endTime;
  		
		getDataList(this.state,()=> {
			message.info('请求成功');
		}, () => {
			message.info('请求失败');
		})
